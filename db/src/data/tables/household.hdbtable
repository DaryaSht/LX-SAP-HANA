COLUMN TABLE "HOUSEHOLD"(
    "HOUSEHOLD_ID" INTEGER NOT NULL COMMENT 'Household ID',
    "HOUSEHOLD_NAME" CHAR(30) COMMENT 'Family Name',
    "HOUSEHILD_ADDRESS" CHAR(50) COMMENT 'Address',
    -- "HOUSEHILD_SIZE" AS COUNT_ROWS( "HOBBIT"."HOUSEHOLD_ID" = "HOUSEHOLD_ID"),
    PRIMARY KEY ("HOUSEHOLD_ID")
)