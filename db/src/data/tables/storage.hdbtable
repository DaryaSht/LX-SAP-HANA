COLUMN TABLE "STORAGE"(
    "STORAGE_ID" INTEGER NOT NULL COMMENT 'Storage ID',
    "STORAGE_NAME" CHAR(30) COMMENT 'Storage Name',
    "LOCATION" CHAR(30) COMMENT 'Location',
    "HOUSEHOLD_ID" INTEGER NOT NULL COMMENT 'Household ID',
    PRIMARY KEY ("STORAGE_ID")
) WITH ASSOCIATIONS ( JOIN "HOUSEHOLD" AS "HOUSEHOLD_JOIN" ON "HOUSEHOLD_ID" = "HOUSEHOLD_JOIN"."HOUSEHOLD_ID" )