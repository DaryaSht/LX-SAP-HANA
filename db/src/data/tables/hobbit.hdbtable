COLUMN TABLE "HOBBIT"(
    "HOBBIT_ID" INTEGER NOT NULL COMMENT 'Hobbit ID',
    "FIRST_NAME" CHAR(20) NOT NULL COMMENT 'Hobbit Name',
    "LAST_NAME" CHAR(20) NOT NULL COMMENT 'Hobbit Lastname',
    "FULL_NAME" AS CONCAT(FIRST_NAME, CONCAT(' ',LAST_NAME)),
    "BIRTH_DATE" DATE COMMENT 'Birthday',
    "HOUSEHOLD_ID" INTEGER NOT NULL COMMENT 'Household ID',
    PRIMARY KEY ("HOBBIT_ID")
) WITH ASSOCIATIONS ( JOIN "HOUSEHOLD" AS "HOUSEHOLD_JOIN" ON "HOUSEHOLD_ID" = "HOUSEHOLD_JOIN"."HOUSEHOLD_ID" )